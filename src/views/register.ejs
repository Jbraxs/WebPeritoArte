            <label>Contraseña</label>
            <input type="password" class="form-control" data-validation="length" data-validation-length="min6" name="password">
          </div>
          <div class="form-group">
            <label>Teléfono</label>
            <input type="text" class="form-control" data-validation="required number" name="telefono">  
          </div>
          <div class="form-group">
            <label>Código Postal</label>
