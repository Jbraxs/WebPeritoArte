<% include templates/_header %>
  Listado de valoraciones de los clientes registrados
  <div class="container mt-5">
    <div class="row">
      <div class="col-md-7">
        <table class="table table-bordered table-hover">
          <thead>
            <tr>
              <td>Id Valoraciones</td>
              <td>Id Cliente</td>
              <td>Nombre</td>
              <td>Tipo  </td>
              <td>Técnica</td>
              <td>Tamaño</td>
              <td>Estado</td>
              <td>Firmado</td>
              <td>Comentario</td>
              <td>Imagenes</td>
            </tr>
          </thead>
          <tbody>
            <% if (data) { %>
              <% for(var i = 0; i < data.length; i++) { %>
                <tr>
                  <td>
                    <%= data[i].idObjeto %>
                  </td>
                  <td>
                    <%= data[i].id %>
                  </td>
                  <td>
                    <%= data[i].nombre %>
                  </td>
                  <td>
                    <%= data[i].tipo %>
                  </td>
                  <td>
                    <%= data[i].tecnica %>
                  </td>
                  <td>
                    <%= data[i].tamanio %>
                  </td>
                  <td>
                    <%= data[i].estado %>
                  </td>
                  <td>
                    <%= data[i].firmado %>
                  </td>
                  <td>
                    <%= data[i].comentario %>
                  </td>
                  <td>
                    <%= data[i].imagenes %>
                  </td>
                  <td>
                    <a href="/ <%= data[i].id %>" class="btn btn-info">Peritar</a>
                    <a href="/ <%= data[i].id %>" class="btn btn-danger">Delete</a>
                  </td>
                </tr>
                <% } %>
                  <% } %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
  <% include templates/_footer %>