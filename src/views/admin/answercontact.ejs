<% include ../partials/_headeradmin %>


    <h1>RESPUESTA AL FORMULARIO</h1>
    <div class="col-md-5">
        <div class="card-body">
            <form action="/contact" method="POST">
                <div class="form-group">
                    <label>Nombre</label>
                    <p class="form-control-static"><%= data.nombre%></p>
                </div>
                <div class="form-group">
                    <label>Email</label>
                    <p class="form-control-static"><%= data.email %></p>
                </div>
                <div class="form-group">
                    <label>Mensaje de Respuesta</label>
                    <textarea name="comentario" rows="15" cols="30"  class="form-control">Deje su mensaje</textarea>
                </div>
                <button type="submit" class="btn btn-primary">
                    Enviar
                </button>
            </form>
        </div>
    </div>