<% include ../partials/_header %>
<% include ../partials/_bar %>
<% include ../partials/_nav %>


<div class="content-wrapper">
  <section class="content">
    <div class="row">
      <div class="col-xs-12">
        <div class="box">
          <div class="box-header">
          </div>
          <div class="box-body">
            <table id="usuarios" class="table table-bordered table-striped">
              <thead>
                <tr>
                  <th>Nombre</th>
                  <th>Apellido</th>
                  <th>Email</th>
                  <th>Télefono</th>
                  <th>Comentario</th>
                  <th>Acción</th>
                </tr>
              </thead>
              <tbody>
                  <% if (data) { %>
                    <% for(var i = 0; i < data.length; i++) { %>
                <tr>
                    <td>
                        <%= data[i].nombre %>
                      </td>
                      <td>
                        <%= data[i].apellido %>
                      </td>
                      <td>
                        <%= data[i].email %>
                      </td>
                      <td>
                        <%= data[i].telefono %>
                      </td>
                      <td>
                        <%= data[i].comentario %>
                      </td>
                      <td>
                          <a href="/admin/contacts/view/<%= data[i].id %>"  class="btn btn-success btn-sm">Ver</a>
                          <a href="/admin/contacts/delete/<%= data[i].id %>" class="btn btn-danger btn-sm">Eliminar</a>
                        </td>
                      </tr>
                      <% } %>
                        <% } %>
              </tbody>
              <tfoot>
              </tfoot>
            </table>
          </div>
        </div>
      </div>
    </div>
  </section>
</div>



<% include ../partials/_footer %>