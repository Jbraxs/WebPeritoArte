<!-- RECUPERAR CONTRASEÃ‘A ERRORES-->

<% if (messages.notices) { %>
<div class="alert alert-primary" role="alert">
<% messages.notices.forEach(function(notices) { %>
    <%= notices %>
<% }) %>
</div>
<% } %>


<% if (messages.errors) { %>
<% messages.errors.forEach(function(errors) { %>
<div class="alert alert-danger" role="alert">
    <%= errors %>
<% }) %>
</div>
<% } %>


